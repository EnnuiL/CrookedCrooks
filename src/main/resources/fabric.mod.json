  "authors": ["EnnuiL"],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/crooked-crooks",
    "issues": "https://github.com/EnnuiL/CrookedCrooks/issues",
    "sources": "https://github.com/EnnuiL/CrookedCrooks"
  },
  "license": "MIT",
  "icon": "assets/crookedcrooks/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": ["io.github.ennuil.crookedcrooks.CrookedCrooksMod"],
    "libcd:conditions": ["io.github.ennuil.crookedcrooks.CrookedCrooksConditions"]
  },
  "mixins": [
    "crookedcrooks.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": ">=0.38.0",
    "minecraft": ">=1.17.0",
    "libcd": ">=2.5.0"
  },
  "recommends": {
    "libcd": ">=3.0.0"
  },
  "suggests": {
